{
    "name": "elegant",
    "version": "0.1.0",
    "scripts": {
        "dev": "next",
        "build": "next build && next export",
        "start": "next start",
        "find:unused": "next-unused"
    },
    "next-unused": {
        "alias": {
            "@lib/*": [
                "lib/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@config/*": [
                "config/*"
            ],
            "@components/*": [
                "components/*"
            ]
        },
        "debug": true,
        "include": [
            "components",
            "lib",
            "pages"
        ],
        "exclude": [],
        "entrypoints": [
            "pages"
        ]
    },
    "browser": {
        "fs": false,
        "net": false,
        "tls": false
    },
